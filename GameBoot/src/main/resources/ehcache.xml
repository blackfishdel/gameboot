<config xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xmlns='http://www.ehcache.org/v3' xmlns:jsr107='http://www.ehcache.org/v3/jsr107'
	xsi:schemaLocation="http://www.ehcache.org/v3 ../../../../../../../xml/src/main/resources/ehcache-core.xsd
                        http://www.ehcache.org/v3/jsr107 ../../../../../main/resources/ehcache-107ext.xsd">

	<cache alias="sessions">
	
		<key-type>java.lang.String</key-type>
		<value-type>java.util.List</value-type>

		<expiry>
			<tti unit="seconds">15</tti>
		</expiry>

		<heap size="1" unit="entries" />
	</cache>

</config>